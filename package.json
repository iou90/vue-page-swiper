{
  "name": "vue-page-swiper",
  "version": "1.0.0",
  "description": "vue3 & vue router 4 page swiper with navigation direction specific transitions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iou90/vue-page-swiper.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "swiper"
  ],
  "author": "iou90 <iou90@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iou90/vue-page-swiper/issues"
  },
  "homepage": "https://github.com/iou90/vue-page-swiper#readme",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/vue-page-swiper.umd.cjs",
  "module": "./dist/vue-page-swiper.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-page-swiper.js",
      "require": "./dist/vue-page-swiper.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --emitDeclarationOnly",
    "lint:fix": "eslint --ext .ts,.vue . --fix --cache"
  },
  "lint-staged": {
    "*.(ts|vue)": [
      "npm run lint:fix",
      "git add -u"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.16.0",
    "husky": "4.3.8",
    "lint-staged": "11.2.6",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  }
}
